[ui]
username = Brad Robel-Forrest <brad@unity3d.com>
merge = internal:merge3
# merge = emacs

[extensions]
largefiles =
eol =
rebase =
transplant =
progress =
purge =
strip =
# os x install in _THIS_ order (system, not user for pip!):
#   > pip install keyring
#   > pip show keyring
#   > pip install mercurial_extension_utils
#   > pip install mercurial_keyring
#   > brew install mercurial
mercurial_keyring =
dynapath = ~/work/dynapath/dynapath.py
color =
pager =
shelve=

[auth]
ono.schemes = https
ono.prefix = ono.unity3d.com
ono.username = brad

ono-proxy.schemes = https
ono-proxy.prefix = ono-proxy-seattle.hq.unity3d.com
ono-proxy.username = brad

# [merge-tools]
# emacs.args = -q --eval "(ediff-merge-with-ancestor \""$local"\" \""$other"\" \""$base"\" nil \""$output"\")"

[diff]
git = True

[color]
diff.trailingwhitespace = bold red_background
diff.diffline = bold
diff.extended = magenta bold
diff.file_a = red bold
diff.file_b = green bold
diff.hunk = cyan
diff.deleted = red
diff.inserted = green
diff.changed = white

[pager]
pager = LESS='FRX' less
# ignore = version, help, update

[dynapath]
seattle-mirror.ipprefix = 10.1.16.0/22
seattle-mirror.pathprefix = https://ono.unity3d.com/
seattle-mirror.pathsubst = https://ono-proxy-seattle.hq.unity3d.com/

